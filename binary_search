#include <stdio.h>
int main() {
    int n, a[100], key, low = 0, high, mid, i, f = 0;
    printf("Enter number of students: ");
    scanf("%d", &n);
    printf("Enter sorted roll numbers:\n");
    for(i = 0; i < n; i++) scanf("%d", &a[i]);
    printf("Enter roll to search: ");
    scanf("%d", &key);
    high = n - 1;
    while(low <= high) {
        mid = (low + high) / 2;
        if(a[mid] == key) { f = 1; break; }
        else if(a[mid] < key) low = mid + 1;
        else high = mid - 1;
    }
    printf(f ? "Student attended.\n" : "Student not attended.\n");
    return 0;
}
